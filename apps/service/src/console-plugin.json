{
  "extensions": [
    {
      "type": "console-platform/routes/feature",
      "options": {
        "path": "helloworld",
        "expose": "./Routes"
      }
    },
    {
      "type": "console-platform/navigation",
      "options": {
        "name": "helloworld",
        "displayName": { "en": "Hello World", "zh": "你好，世界" },
        "href": "$cluster/helloworld",
        "insertBefore": "overview"
      }
    },
    {
      "type": "console-acp/routes/feature",
      "options": {
        "path": "helloworld"
      }
    },
    {
      "type": "console-acp/navigation/group",
      "options": {
        "group": "namespace_scope",
        "name": "helloworld",
        "displayName": { "en": "Hello World", "zh": "你好，世界" },
        "href": "helloworld",
        "insertBefore": "overview"
      }
    }
  ]
}
